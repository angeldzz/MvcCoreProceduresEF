@model IEnumerable<MvcCoreProceduresEF.Models.Enfermo>


<h1>Enfermos</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Apellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaNacimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Genero)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nss)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaNacimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Genero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nss)
            </td>
            <td>
                    <a asp-action="Edit" asp-route-inscripcion="@item.Inscripcion">Edit</a> |
                    <a asp-action="Details" asp-route-inscripcion="@item.Inscripcion">Details</a> |
                    <a asp-action="DeleteRaw" asp-route-inscripcion="@item.Inscripcion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
